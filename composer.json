{
    "name": "ipc/tdd",
    "description": "Test Driven Domains @ IPC 2015",
    "authors": [
        {
            "name": "Nikolai Zujev",
            "email": "nikolai.zujev@gmail.com"
        }
    ],
    "require": {
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "phpunit/phpunit-mock-objects": "~2.2",
        "phpunit/php-code-coverage": "~2.0",
        "phpunit/php-token-stream": "~1.3",
        "phpunit/php-invoker": "~1.1"
    },
    "autoload": {
	    "psr-4": {
            "Workshop\\Auction\\": "src/"
	    }
    },
    "config": {
        "github-protocols": ["git"],
        "bin-dir": "bin",
        "vendor-dir": "vendor",
        "optimize-autoloader": true,
        "preferred-install": "src",
        "store-auths": true,
        "notify-on-install": false
    },
    "scripts": {
        "test": "phpunit"
    }
}
